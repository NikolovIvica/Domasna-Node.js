<!DOCTYPE html>
<html>

<head>

</head>

<body>
    <a href="/create">Create new user</a>
    <h2>Table of users</h2>
    <table>
     <% users.forEach(user => { %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.lastname %></td>
        <td><%= user.ssn %></td>
        <td><a href="/user/<%= user.id %>">View</a></td>
      </tr>
     <% }) %>
    </table>
</body>

</html>